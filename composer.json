{
  "name": "gugler/gugler-powermail",
  "description": "A11y improvements for powermail extension",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Ing. Markus Ketterer",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Gugler\\GuglerPowermail\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/gugler_powermail": "self.version",
    "gugler_powermail": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "gugler_powermail"
    }
  }
}
